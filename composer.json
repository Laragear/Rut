{
    "name": "laragear/rut",
    "description": "Tools to parse, validate and generate Chilean RUT in Laravel.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "rut",
        "chile"
    ],
    "authors": [
        {
            "name": "Italo Baeza C.",
            "email": "DarkGhostHunter@Gmail.com",
            "homepage": "https://github.com/sponsors/DarkGhostHunter"
        }
    ],
    "support": {
        "source": "https://github.com/Laragear/Rut",
        "issues": "https://github.com/Laragear/Rut/issues"
    },
    "require": {
        "php": "8.*",
        "illuminate/database": "9.*|10.*",
        "illuminate/validation": "9.*|10.*",
        "illuminate/support": "9.*|10.*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.22|8.*",
        "doctrine/dbal": "^3.3",
        "jetbrains/phpstorm-attributes": "*"
    },
    "autoload": {
        "psr-4": {
            "Laragear\\Rut\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laragear\\Rut\\RutServiceProvider"
            ],
            "aliases": {
                "Generator": "Laragear\\Rut\\Facades\\Generator"
            }
        }
    },
    "funding": [
        {
            "type": "GitHub Sponsorship",
            "url": "https://github.com/sponsors/DarkGhostHunter"
        },
        {
            "type": "Paypal",
            "url": "https://paypal.me/darkghosthunter"
        }
    ]
}
