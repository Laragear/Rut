{
  "name": "laragear/rut",
  "description": "RUT utilities for modern PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Italo Baeza C.",
      "email": "DarkGhostHunter@Gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/laragear/rut"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "illuminate/validation": "^8.67",
    "illuminate/support": "^8.67"
  },
  "require-dev": {
    "orchestra/testbench": "^6.23",
    "phpunit/phpunit": "^9.5.10",
    "mockery/mockery": "^1.4.4",
    "doctrine/dbal": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Laragear\\Rut\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laragear\\Rut\\RutServiceProvider"
      ],
      "aliases": {
        "Generator": "Laragear\\Rut\\Facades\\Generator"
      }
    }
  },
  "suggest": {
    "laravel/framework": "For your application needs."
  },
  "funding": [
    {
      "type": "Ko-Fi",
      "url": "https://ko-fi.com/DarkGhostHunter"
    },
    {
      "type": "Paypal",
      "url": "https://paypal.me/darkghosthunter"
    }
  ]
}
